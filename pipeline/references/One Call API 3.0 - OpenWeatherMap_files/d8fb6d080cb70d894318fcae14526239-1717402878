hljs.registerLanguage("yaml",function(){"use strict";return function(e){var n="true false yes no null",a="[\\w#;/?:@&=+$,.~*\\'()[\\]]+",s={className:"string",relevance:0,variants:[{begin:/'/,end:/'/},{begin:/"/,end:/"/},{begin:/\S+/}],contains:[e.BACKSLASH_ESCAPE,{className:"template-variable",variants:[{begin:"{{",end:"}}"},{begin:"%{",end:"}"}]}]},i=e.inherit(s,{variants:[{begin:/'/,end:/'/},{begin:/"/,end:/"/},{begin:/[^\s,{}[\]]+/}]}),l={end:",",endsWithParent:!0,excludeEnd:!0,contains:[],keywords:n,relevance:0},t={begin:"{",end:"}",contains:[l],illegal:"\\n",relevance:0},g={begin:"\\[",end:"\\]",contains:[l],illegal:"\\n",relevance:0},b=[{className:"attr",variants:[{begin:"\\w[\\w :\\/.-]*:(?=[ \t]|$)"},{begin:'"\\w[\\w :\\/.-]*":(?=[ \t]|$)'},{begin:"'\\w[\\w :\\/.-]*':(?=[ \t]|$)"}]},{className:"meta",begin:"^---s*$",relevance:10},{className:"string",begin:"[\\|>]([0-9]?[+-])?[ ]*\\n( *)[\\S ]+\\n(\\2[\\S ]+\\n?)*"},{begin:"<%[%=-]?",end:"[%-]?%>",subLanguage:"ruby",excludeBegin:!0,excludeEnd:!0,relevance:0},{className:"type",begin:"!\\w+!"+a},{className:"type",begin:"!<"+a+">"},{className:"type",begin:"!"+a},{className:"type",begin:"!!"+a},{className:"meta",begin:"&"+e.UNDERSCORE_IDENT_RE+"$"},{className:"meta",begin:"\\*"+e.UNDERSCORE_IDENT_RE+"$"},{className:"bullet",begin:"\\-(?=[ ]|$)",relevance:0},e.HASH_COMMENT_MODE,{beginKeywords:n,keywords:{literal:n}},{className:"number",begin:"\\b[0-9]{4}(-[0-9][0-9]){0,2}([Tt \\t][0-9][0-9]?(:[0-9][0-9]){2})?(\\.[0-9]*)?([ \\t])*(Z|[-+][0-9][0-9]?(:[0-9][0-9])?)?\\b"},{className:"number",begin:e.C_NUMBER_RE+"\\b"},t,g,s],c=[...b];return c.pop(),c.push(i),l.contains=c,{name:"YAML",case_insensitive:!0,aliases:["yml","YAML"],contains:b}}}());
const url = "https://home.openweathermap.org/pricing";
const placeholder = "By request";
const period = " month";

function composePrice (val, currency) {
    return '<b>' + val + " " + currency + '</b><small>/' + period + '</small>'
}

function composeOnecallPrice (val, currency) {
    return '<b>' + val + " " + currency + '</b>'
}

function composeEnergyPrice (val, currency) {
    return '<b>' + val + " " + currency + '</b>'
}

function composeBrPrice (val, currency) {
    return '<br /><b>' + val + " " + currency + '</b><small>/' + period + '</small>'
}

function composeFromPrice (val, currency) {
    return '<small>from </small><br /><b>' + val + " " + currency + '</b><small>/' + period + '</small>'
}

fetch(url)
    .then(response => response.json())
    .then(data => {

        let solar_radiation, global_alerts;
        if (data.prices && data.currency) {

            let currency = data.currency;
            let weather = data.prices.weather;
            let onecall_30 = data.prices.onecall_30;
            let energy = data.prices.energy;
            let history = data.prices.history;
            let tailored = data.prices.tailored;

            if (onecall_30.base) {
                elem = document.getElementById("price_onecall_30")
                if (elem) {
                    elem.innerHTML = composeOnecallPrice(onecall_30.base, currency);
                }
            }

            if (energy.base) {
                elem = document.getElementById("price_energy")
                if (elem) {
                    elem.innerHTML = composeEnergyPrice(energy.base, currency);
                }
            }

            if (energy.location) {
                elem = document.getElementById("price_energy_location")
                if (elem) {
                    elem.innerHTML = composeEnergyPrice(energy.location, currency);
                }
            }

            if (weather.startup) {
                elem = document.getElementById("price_startup")
                if (elem) {
                    elem.innerHTML = composePrice(weather.startup, currency);
                }
            }

            if (weather.dev) {
                elem = document.getElementById("price_dev")
                if (elem) {
                    elem.innerHTML = composePrice(weather.dev, currency);
                }
            }

            if (weather.pro) {
                elem = document.getElementById("price_pro")
                if (elem) {
                    elem.innerHTML = composePrice(weather.pro, currency);
                }
            }

            if (weather.ent) {
                elem = document.getElementById("price_ent")
                if (elem) {
                    elem.innerHTML = composePrice(weather.ent, currency);
                }
            }

            if (history.start) {
                elem = document.getElementById("price_start")
                if (elem) {
                    elem.innerHTML = composePrice(history.start, currency);
                }
            }

            if (history.med) {
                elem = document.getElementById("price_med")
                if (elem) {
                    elem.innerHTML = composePrice(history.med, currency);
                }
            }


            elem = document.getElementById("price_adv")
            if (elem) {
                elem.innerHTML = history.adv ? composePrice(history.adv, currency) : placeholder;
            }


            if (history.hb) {
                elem = document.getElementById("price_hb");
                if (elem) {
                    elem.innerHTML = '<b>' + history.hb + " " + currency + '</b><small>/ per location</small>';
                }
            }

            if (history.hfb) {
                elem = document.getElementById("price_hfb");
                if (elem) {
                    elem.innerHTML = '<b>' + history.hfb + " " + currency + '</b><small>/ per location</small>';
                }
            }


            if (tailored.global_alerts) {
                elem = document.getElementById("price_global_alerts");
                if (elem) {
                    elem.innerHTML = composePrice(tailored.global_alerts, currency);
                }
            }


        } else {
            elem = document.getElementById("price_solar_radiation")
            if (elem) {
                elem.innerHTML = placeholder;
            }

            elem =  document.getElementById("price_global_alerts")
            if (elem) {
                elem.innerHTML = placeholder;
            }
        }
    })
    .catch(err => {
        console.log(err);

        elem = document.getElementById("price_solar_radiation")
        if (elem) {
            elem.innerHTML = placeholder;
        }

        elem =  document.getElementById("price_global_alerts")
        if (elem) {
            elem.innerHTML = placeholder;
        }

        elem =  document.getElementById("price_adv")
        if (elem) {
            elem.innerHTML = '';
        }
    })
function confSubmit()
{
    if(!document.getElementById("one").checked)
    {
        alert("Please read and accept the Terms of Service and Privacy Policy");
        return false;
    }

    if(document.getElementById("input_appid").value.trim().length != 32)
    {
        alert("Please use correct API key (32 symbols)");
        return false
    }
}
